-- corner
local corner2 = w("UICorner")
corner2.CornerRadius = UDim.new(0, 10)
corner2.Parent = pic
-- most important buttons here
local slapsec = w("TextButton")
slapsec.Name = "slapsection"
slapsec.Text = "SLAP"
slapsec.Size = UDim2.new(0.04, 0, 0.1, 0)
slapsec.Position = UDim2.new(0.72, 0, 0.25)
slapsec.BackgroundTransparency = 0.5
slapsec.TextColor3 = Color3.new(255, 255, 255)
slapsec.Parent = mainbook
-- badge section
local badge = w("TextButton")
badge.Name = "badgesection"
badge.Text = "BADGE"
badge.Size = UDim2.new(0.04, 0, 0.1, 0)
badge.Position = UDim2.new(0.72, 0, 0.38)
badge.TextSize = 6
badge.BackgroundTransparency = 0.5
badge.TextColor3 = Color3.new(255, 255, 255)
badge.Parent = mainbook
-- border
slapsec.BorderSizePixel = 3
slapsec.BorderColor3 = Color3.new(255, 255, 255)
--another frame
local fuck = w("ScrollingFrame")
fuck.BackgroundTransparency = 1
fuck.Name = "badgegloves"
fuck.Size = UDim2.new(0.5, 0, 0.8, 0)
fuck.Position = UDim2.new(0.2, 0, 0.1)
fuck.Active = true
fuck.Parent = mainbook
fuck.Visible = false
local tester = game.Players.LocalPlayer.PlayerGui.noidea.badgegloves
-- and afterall we need a hide button dont we
local hide = w('TextButton')
hide.Name = 'hiding'
hide.Text = 'GUI'
hide.Size = UDim2.new(0.05, 0, 0.11, 0)
hide.TextColor3 = Color3.new(255, 255, 255)
hide.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
hide.Font = Enum.Font.AmaticSC
hide.TextSize = 25
hide.BackgroundTransparency = 0.5
hide.Position = UDim2.new(0.93, 0, 0.08, 0)
hide.Parent = mainbook
local hidec = w('UICorner')
hidec.CornerRadius = UDim.new(0, 20)
hidec.Parent = hide
hide.Visible = false
local closebutton = w("TextButton")
closebutton.Name = "close"
closebutton.Text = "X"
closebutton.TextSize = 15
closebutton.Size = UDim2.new(0.03, 0, 0.07, 0)
closebutton.Position = UDim2.new(0.73, 0, 0.11)
closebutton.Parent = mainbook
closebutton.TextColor3 = Color3.new(255, 255, 255)
closebutton.BackgroundTransparency = 1
closebutton.MouseButton1Click:Connect(function()
hide.Visible = true
sadness.Visible = false
scrollingFrame.Visible = false
fuck.Visible = false
badge.Visible = false
slapsec.Visible = false
closebutton.Visible = false  
end)
hide.MouseButton1Click:Connect(function()
hide.Visible = false
sadness.Visible = true
scrollingFrame.Visible = true
badge.Visible = true
slapsec.Visible = true
closebutton.Visible = true
badge.BorderSizePixel = 0.1
badge.BorderColor3 = Color3.new(0, 0, 0)  
slapsec.BorderSizePixel = 3 
slapsec.BorderColor3 = Color3.new(255, 255, 255)  
end)
-- rest from now
badge.MouseButton1Click:Connect(function()
badge.BorderSizePixel = 3
  wait()
  badge.BorderColor3 = Color3.new(255, 255, 255)
  wait()
  slapsec.BorderSizePixel = 0.1
  slapsec.BorderColor3 = Color3.new(0, 0, 0)
  wait()
  scrollingFrame.Visible = false
  wait()
  fuck.Visible = true
end)
slapsec.MouseButton1Click:Connect(function()
slapsec.BorderSizePixel = 3
slapsec.BorderColor3 = Color3.new(255, 255, 255)
wait()
badge.BorderSizePixel = 0.1
badge.BorderColor3 = Color3.new(0, 0, 0)
wait()
fuck.Visible = false
wait()
scrollingFrame.Visible = true
end)

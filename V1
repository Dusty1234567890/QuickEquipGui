-- scripting of buttons part
OVERKILL.MouseButton1Click:Connect(function()
  lobby = game.Players.LocalPlayer.Character:FindFirstChild("InLobby")
  if lobby then
    if gloves.OVERKILL.Value == true then
fireclickdetector(game.workspace.Lobby.OVERKILL.ClickDetector)
    else
CustomSendNotification("info", "you dont own this glove", "rbxassetid://17105411523")
    end
elseif lobby == nil then
CustomSendNotification("info", "you're not in lobby", "rbxassetid://17105411523")
end
end)
